	if (!(str = (char *)ft_malloc(sizeof(char) * ((end + 1) - start) + 1)))
		return (0);
	while (start < (end + 1))
	{
		str[i] = s1[start];
		i++;
		start++;
	}
	str[i] = '\0';
	return (str);
}

char			*ft_strtrim(char const *s1, char const *set)
{
	int		start;
	int		end;
	
	start = 0;
	if (!s1)
		return (0);
	if (set == 0)
		return (ft_strdup(s1));
	end = ft_strlen(s1);
	while (s1[start])
	{
		if (0 == (ft_strchr(set, s1[start])))
			break ;
		start++;
	}
	while (s1[end] != s1[start])
	{
		if (0 == (ft_strchr(set, s1[end])))
			break ;
		end--;
	}
	return (display(s1, start, end));
}

