char	*rstr_to_cstr(t_rstr rs)
{
	int		i;
	char	*str;

	i = 0;
	str = malloc(rs->len + 1);
	while (i < rs->len)
	{
		str[i] = rs->data[i];
		i++;
	}
	str[rs->len] = '\0';
	return (str);
}

char    rstr_get(t_rstr rs, int index)
{
        if (index < 0 || index >= rs->len)
                return (-42);
        return (*(rs->data + index));
}

t_rstr			cstr_to_rstr(char *cstr)
{
	t_rstr rs;
	int		i;

	i = 0;
	rs = rstr_create(0);
	while (cstr[i])
	{
		rstr_add(rs, cstr[i]);
		i++;
	}
	return (rs);
}
