char		*ft_substr(char const *s, unsigned int start, size_t len)
{
	char	*ptr;
	size_t	i;

	i = 0;
	if (!s)
		return (0);
	if (start > len)
		return (ft_strdup(""));
	if (!(ptr = (char *)malloc(sizeof(char) * (len) + 1)))
