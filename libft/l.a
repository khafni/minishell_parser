#include "libft.h"

void	*ft_memmove(void *dst, const void *src, size_t len)
{
	char		*tdst;
	const char	*tsrc;
	char		*enddst;
	const char	*endsrc;

	tdst = dst;
	tsrc = src;
	enddst = tdst + (len - 1);
	endsrc = tsrc + (len - 1);
	if (src == 0 && dst == 0)
		return (0);
	if ((tdst < tsrc) || (dst == src && len == 1))
		return (ft_memcpy(dst, src, len));
	while (len-- > 0)
		*enddst-- = *endsrc--;
	return (dst);
}

