void	*ft_memchr(const void *s, int c, size_t n)
{
	unsigned char *str;

	str = (unsigned char *)s;
	while (n)
	{
		if (*str != (unsigned char)c)
			str++;
		else
			return ((void *)str);
		n--;
	}
	return (0);
}
