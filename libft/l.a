#include "libft.h"

void	*ft_memcpy(void *dst, const void *src, size_t n)
{
	char		*tdst;
	const char	*tsrc;

	tdst = dst;
	tsrc = src;
	if (dst == src && n == 1)
		return (dst);
	if (!tsrc && !tdst)
		return (0);
	while (n > 0)
	{
		*tdst++ = *tsrc++;
		n--;
	}
	return (dst);
}
